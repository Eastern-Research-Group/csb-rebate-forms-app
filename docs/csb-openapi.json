    "/api/config": {
      "get": {
        "summary": "Get CSB app specific configuration (form open enrollment status, etc.)",
        "parameters": [],
        "responses": {
          "200": {
            "description": "Open enrollment status (true/false) for each form, by rebate year.",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "submissionPeriodOpen": {
                      "type": "object",
                      "properties": {
                        "2022": {
                          "type": "object",
                          "properties": {
                            "frf": {
                              "type": "boolean",
                              "example": true
                            },
                            "prf": {
                              "type": "boolean",
                              "example": true
                            },
                            "crf": {
                              "type": "boolean",
                              "example": true
                            }
                          }
                        },
                        "2023": {
                          "type": "object",
                          "properties": {
                            "frf": {
                              "type": "boolean",
                              "example": true
                            },
                            "prf": {
                              "type": "boolean",
                              "example": true
                            },
                            "crf": {
                              "type": "boolean",
                              "example": true
                            }
                          }
                        }
                      }
                    }
                  }
                }
              }
            }
